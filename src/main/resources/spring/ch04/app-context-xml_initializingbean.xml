<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd" default-lazy-init="true">

    <bean id="singerOne" class="ru.smith.ch04.xml.initializingbean.SingerWithInterface" p:name="John Mayer" p:age="39"/>
    <bean id="singerTwo" class="ru.smith.ch04.xml.initializingbean.SingerWithInterface" p:age="75"/>
    <bean id="singerThree" class="ru.smith.ch04.xml.initializingbean.SingerWithInterface" p:name="John Butler"/>

</beans>